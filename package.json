    "eslint": "^7.8.1",
    "ethereumjs-wallet": "^1.0.1",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.10",
