    "@poofcash/poof-token": "^1.0.6",
    "@types/ethereumjs-abi": "^0.6.3",
    "bip39": "^3.0.4",
    "solparse": "^2.2.8",
    "ts-node": "^10.0.0",
    "web3": "^1.3.6",
    "web3-utils": "^1.3.6"
  },
  "devDependencies": {
    "@celo/typechain-target-web3-v1-celo": "^0.2.0",
